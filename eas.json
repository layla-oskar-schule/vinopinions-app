// https://docs.expo.dev/build/eas-json/
{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "extends": "production",
      "env": {
        "API_URL": "https://api-dev.vinopinions.app/v1"
      }
    },
    "production": {
      "channel": "production",
      "node": "20.11.1",
      "env": {
        "API_URL": "https://api.vinopinions.app/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
